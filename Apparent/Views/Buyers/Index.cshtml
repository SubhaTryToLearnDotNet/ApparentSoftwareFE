                            string image = Model.DtProductList.Rows[s]["ProductIcon"].ToString();
                            <div class="col-sm-6 col-md-4-5 col-lg-1-5">

                                <a href="/Product/Details?ProductId=@Model.DtProductList.Rows[s]["ProductId"].ToString()">
