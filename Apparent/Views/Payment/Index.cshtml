        var PlanId = $('input[name="PlanId"]').val();
        var Plan_Tenure = $('input[name="Plan_Tenure"]').val();
        var ProductId = $('input[name="ProductId"]').val();
        if (card_number == '') {
            swal({
                title: "Oops!",
                text: "Enter card number",
                type: "error"
            });
            return;
        }
        else if (month == '') {
            swal({
                title: "Oops!",
                text: "Select expiry month",
                type: "error"
            });
            return;
        }
        else if (year == '') {
            swal({
                title: "Oops!",
                text: "Select expiry year",
                type: "error"
            });
            return;
        }
        else if (card_holder == '') {
            swal({
                title: "Oops!",
                text: "Enter card holder",
                type: "error"
            });
            return;
        }

        document.body.classList.add("loading")//Loader start
        const fd = new FormData();
        fd.append('card_number', card_number);
        fd.append('card_holder', card_holder);
        fd.append('cvv', cvv);
        fd.append('month', month);
        fd.append('year', year);
        fd.append('amount', amount);
        fd.append('ProductId', ProductId);
        fd.append('Plan_Type', Plan_Type);
        fd.append('Plan_Tenure', Plan_Tenure);
        fd.append('PlanId', PlanId);
        fd.append('token', $('input[name="token"]').val());
        $.ajax({
            url: '/Payment/card_payment',
            method: 'POST',
            async: true,
            contentType: false,
            cache: false,
            processData: false,
            data: fd,
            success: function (data) {
                document.body.classList.remove("loading")//Loader stop
                if (data.code == 200 && data.msg == 'Payment Successfully Done.') {

                    swal({
                        title: "Success!",
                        text: data.msg,
                        type: "success"
                    }, function () {

                        window.location = "/Profile/my_product";
                    });

                }
                else if (data.code == 200 && data.msg == 'Declined') {
                    swal({
                        title: "Oops!",
                        text: data.msg,
                        type: "error"
                    });
                }
                else if (data.code == 400) {
                    swal({
                        title: "Oops!",
                        text: data.msg,
                        type: "error"
                    });
                }

                else if (data.code == 500) {
                    swal({
                        title: "Oops!",
                        text: "Something went wrong!",
                        type: "error"
                    }, function () {

                        window.location = data.url;
                    });
                }

            }
        });



    }

